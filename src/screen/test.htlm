            width: "240px",
            resizable: false,
            modal: true,
            animation: {
              open: {
                effects: "fade:in"
              },
              close: {
                effects: "fade:out"
              }
            },
            open: function() {
              $('body').addClass('blur');
            },
            close: function() {
              $('body').removeClass('blur');
            }
          });
        

